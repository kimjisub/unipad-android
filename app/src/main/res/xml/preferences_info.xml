<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

	<PreferenceCategory
		app:iconSpaceReserved="false"
		app:title="@string/settings_app">

		<Preference
			app:iconSpaceReserved="false"
			app:key="copyright"
			app:summary="@string/copyright"
			app:title="@string/unipad" />

		<Preference
			app:iconSpaceReserved="false"
			app:key="oss_licence"
			app:title="@string/openSourceLicense" />

		<Preference
			app:iconSpaceReserved="false"
			app:key="language"
			app:summary="@string/languageCode"
			app:title="@string/language" />
	</PreferenceCategory>
	<PreferenceCategory
		app:iconSpaceReserved="false"
		app:title="@string/settings_user">


		<Preference
			app:iconSpaceReserved="false"
			app:key="community"
			app:title="@string/community" />
	</PreferenceCategory>
	<PreferenceCategory
		app:iconSpaceReserved="false"
		app:title="@string/settings_developer">

		<Preference
			app:iconSpaceReserved="false"
			app:key="fcmToken"
			app:title="@string/FCMToken" />
	</PreferenceCategory>

</PreferenceScreen>