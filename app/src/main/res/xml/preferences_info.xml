<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

	<PreferenceCategory
		app:iconSpaceReserved="false"
		app:title="@string/settings_app">

		<Preference
			app:iconSpaceReserved="false"
			app:key="app"
			app:summary="@string/copyright"
			app:title="@string/unipad" />

		<Preference
			app:iconSpaceReserved="false"
			app:key="language"
			app:summary="@string/translated_by"
			app:title="@string/language" />
	</PreferenceCategory>
	<PreferenceCategory
		app:iconSpaceReserved="false"
		app:title="@string/settings_user">

		<com.kimjisub.launchpad.view.preference.IconsPreference
			app:iconSpaceReserved="false"
			app:key="community"/>


		<Preference
			app:iconSpaceReserved="false"
			app:key="community"
			app:title="@string/community" />
	</PreferenceCategory>
	<PreferenceCategory
		app:iconSpaceReserved="false"
		app:title="@string/settings_developer">
		<Preference
			app:iconSpaceReserved="false"
			app:key="oss_licence"
			app:title="@string/openSourceLicense" />

		<Preference
			app:iconSpaceReserved="false"
			app:key="fcm_token"
			app:title="@string/FCMToken" />
	</PreferenceCategory>

</PreferenceScreen>