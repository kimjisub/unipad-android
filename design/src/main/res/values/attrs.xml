<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="Item">
		<attr name="flagColor" format="reference|integer"/>

		<attr name="title" format="string"/>
		<attr name="subTitle" format="string"/>

		<attr name="LED" format="boolean"/>
		<attr name="AutoPlay" format="boolean"/>

		<attr name="size" format="string"/>
		<attr name="chain" format="string"/>
		<attr name="capacity" format="integer"/>

		<attr name="optionVisibility" format="boolean"/>
	</declare-styleable>

	<declare-styleable name="ItemStore" parent="Item">
		<attr name="flagColor"/>

		<attr name="title"/>
		<attr name="subTitle"/>

		<attr name="LED"/>
		<attr name="AutoPlay"/>

		<attr name="downloadCount" format="integer"/>

		<attr name="optionVisibility"/>
	</declare-styleable>
</resources>